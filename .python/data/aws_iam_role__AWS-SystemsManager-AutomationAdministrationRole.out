resource "aws_iam_role" "AWS-SystemsManager-AutomationAdministrationRole" {
  assume_role_policy    = "{\"Statement\":[{\"Action\":\"sts:AssumeRole\",\"Effect\":\"Allow\",\"Principal\":{\"Service\":\"ssm.amazonaws.com\"}}],\"Version\":\"2012-10-17\"}"
  description           = null
  force_detach_policies = false
  managed_policy_arns   = []
  max_session_duration  = 3600
  name                  = "AWS-SystemsManager-AutomationAdministrationRole"
  name_prefix           = null
  path                  = "/"
  permissions_boundary  = null
  tags                  = {}
  tags_all              = {}
  inline_policy {
    name   = "AssumeRole-AWSSystemsManagerAutomationExecutionRole"
    policy = "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Action\":[\"sts:AssumeRole\"],\"Effect\":\"Allow\",\"Resource\":\"arn:aws:iam::*:role/AWS-SystemsManager-AutomationExecutionRole\"},{\"Action\":[\"organizations:ListAccountsForParent\"],\"Effect\":\"Allow\",\"Resource\":[\"*\"]}]}"
  }
}

