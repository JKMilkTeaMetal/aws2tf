resource "aws_iam_role" "AWS-SystemsManager-AutomationExecutionRole" {
  assume_role_policy    = "{\"Statement\":[{\"Action\":\"sts:AssumeRole\",\"Effect\":\"Allow\",\"Principal\":{\"AWS\":\"arn:aws:iam::566972129213:root\"}},{\"Action\":\"sts:AssumeRole\",\"Effect\":\"Allow\",\"Principal\":{\"Service\":\"ssm.amazonaws.com\"}}],\"Version\":\"2012-10-17\"}"
  description           = null
  force_detach_policies = false
  managed_policy_arns   = ["arn:aws:iam::aws:policy/service-role/AmazonSSMAutomationRole"]
  max_session_duration  = 3600
  name                  = "AWS-SystemsManager-AutomationExecutionRole"
  name_prefix           = null
  path                  = "/"
  permissions_boundary  = null
  tags                  = {}
  tags_all              = {}
  inline_policy {
    name   = "ExecutionPolicy"
    policy = "{\"Version\":\"2012-10-17\",\"Statement\":[{\"Action\":[\"resource-groups:ListGroupResources\",\"tag:GetResources\",\"cloudformation:UpdateTerminationProtection\"],\"Effect\":\"Allow\",\"Resource\":\"*\"},{\"Action\":[\"iam:PassRole\"],\"Effect\":\"Allow\",\"Resource\":\"arn:aws:iam::566972129213:role/AWS-SystemsManager-AutomationExecutionRole\"},{\"Action\":[\"iam:ListRoles\",\"iam:AttachRolePolicy\"],\"Effect\":\"Allow\",\"Resource\":\"*\"}]}"
  }
}

